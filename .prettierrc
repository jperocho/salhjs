{
  "semi": true,
  "singleQuote": true,
  "printWidth": 150,
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "useTabs": true,
        "tabWidth": 4
      }
    },
    {
      "files": "*.json",
      "options": {
        "useTabs": false,
        "tabWidth": 2
      }
    },
    {
      "files": "*.yml",
      "options": {
        "useTabs": true,
        "tabWidth": 2
      }
    }
  ]
}
